<Window x:Class="Backdoor.SelectPayload"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Backdoor"
        mc:Ignorable="d"
        Title="SelectPayload" Height="122" Width="260">
    <Grid Margin="0,0,2,0">
        <Button x:Name="KeyLogger_Btn" Content="키 로거" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="KeyLogger_Btn_Click"/>
        <Button x:Name="ShellOpener_Btn" Content="명령 쉘" HorizontalAlignment="Left" Margin="90,10,0,0" VerticalAlignment="Top" Width="75" Click="ShellOpener_Btn_Click"/>
        <Button x:Name="CustomPayload_Btn" Content="사용자 설정..." HorizontalAlignment="Left" Margin="170,10,0,0" VerticalAlignment="Top" Width="75" Click="CustomPayload_Btn_Click"/>
        <TextBox x:Name="CustomPathBox" HorizontalAlignment="Left" Height="23" Margin="10,35,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="230" ToolTip="사용자 설정 페이로드 경로"/>
        <TextBox x:Name="PayloadClientPathBox" HorizontalAlignment="Left" Height="23" Margin="10,63,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="230" ToolTip="클라이언트 경로"/>

    </Grid>
</Window>
